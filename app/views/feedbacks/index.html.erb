<h1>Listing feedbacks</h1>

<table>
  <tr>
    <th>Asunto</th>
    <th>Contenido</th>
    <th>Feedback type</th>
    <th>Project</th>
    <th>Comentario</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feedbacks.each do |feedback| %>
  <tr>
    <td><%= feedback.asunto %></td>
    <td><%= feedback.contenido %></td>
    <td><%= feedback.feedback_type_id %></td>
    <td><%= feedback.project_id %></td>
    <td><%= feedback.comentario_id %></td>
    <td><%= link_to 'Show', feedback %></td>
    <td><%= link_to 'Edit', edit_feedback_path(feedback) %></td>
    <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Feedback', new_feedback_path %>
