<!-- page header -->
<div class="page_header">
  <h1><%= t('admin.tabs.edit_client') %></h1>

  <div class="add_element">
    <% if @client.projects.count == 0 %>
        <button class="btn btn-link clients-link" data-url="<%= clients_delete_path(@client.id) %>">
          <i class="icon-minus icon-white"> </i> <%= t('client.delete_client') %>
        </button>
    <% end %>
  </div>

</div>
<!--// page header

<!-- page contents -->
<div class="page_content" id="edit_client_data">
  <%= render 'form' %>
</div>
